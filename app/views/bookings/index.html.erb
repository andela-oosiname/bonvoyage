<%= render partial: 'header' %>
  <div class="container bookings-box">
  <% unless flash['success'].blank?  %>
  <div class="alert alert-success success-booking" role="alert">
    <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
    <span class="sr-only">Success:</span>
    <%= flash['success'] %>
  </div>
  <% end %>
    <div class="row all-flight">
      <div class="col-sm-1">
         <span> Ref </span>
      </div>
      <div class="col-sm-1">
         <span> Airline </span>
      </div>
      <div class="col-sm-3">
        <span> From</span>
      </div>
      <div class="col-sm-3">
          <span> To </span>
      </div>
       <div class="col-sm-1">
        <span> Departure Time </span>
      </div>
       <div class="col-sm-1">
        <span> seats </span>
      </div>
      <div class="col-sm-1">
        <span> cost </span>
      </div>
      <div class="col-sm-1">
        <span> </span>
      </div>

    </div>
    <%= render partial: 'partials/userbookings' %>
  </div>

